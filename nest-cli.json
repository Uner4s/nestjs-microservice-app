{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/app-nest/src",
  "monorepo": true,
  "root": "apps/app-nest",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/app-nest/tsconfig.app.json"
  },
  "projects": {
    "app-nest": {
      "type": "application",
      "root": "apps/app-nest",
      "entryFile": "main",
      "sourceRoot": "apps/app-nest/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app-nest/tsconfig.app.json"
      }
    },
    "mail-app": {
      "type": "application",
      "root": "apps/mail-app",
      "entryFile": "main",
      "sourceRoot": "apps/mail-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mail-app/tsconfig.app.json"
      }
    }
  }
}